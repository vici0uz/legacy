<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record id="workshop_autopart_receiving_image_view" model="ir.ui.view">
            <field name="name">workshop.autopart.receiving.img</field>
            <field name="model">workshop.autopart.receiving</field>
            <field name="priority">5</field>
            <field name="inherit_id" ref="workshop.workshop_autopart_receive_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form/sheet/notebook" position="inside">
                    <page string="Images">
                        <field name="multi_images" widget="image_multi"/>
                    </page>
                </xpath>
            </field>

        </record>
    </data>
</openerp>
