        </record>

        <record id="sequence_autopart_receive_no" model="ir.sequence">
            <field name="name">Order Number</field>
            <field name="code">order_no.number</field>
            <field name="prefix">R</field>
            <field name="padding">6</field>
        </record>

    </data>
